TASK 3// Use a break to stop at the first prime number between 1 and 20
for (let num = 2; num <= 20; num++) {
  let isPrime = true;
  for (let i = 2; i < num; i++) {
    if (num % i === 0) {
      isPrime = false;
      break;
    }
  }
  if (isPrime) {
    console.log("First prime number is:", num);
    break;
  }
}

// Use continue to print only odd numbers between 1 and 20
for (let i = 1; i <= 20; i++) {
  if (i % 2 === 0) {
    continue; // skip even numbers
  }
  console.log(i);
}
